{
  "name": "master-atul.github.io",
  "version": "2.0.0",
  "description": "Personal website of Atul R",
  "main": "index.js",
  "scripts": {
    "seperator:main": "======================== MAIN SCRIPTS ==========================================",
    "start": "npm run compile:dev",
    "build": "npm run prepare:dev && npm run compile:prod",
    "seperator:test": "======================== TEST AND LINT SCRIPTS =================================",
    "test": "NODE_ENV='test' jest --verbose --forceExit",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage  && open ./coverage/lcov-report/index.html",
    "lint": "echo 'Running eslint' && eslint app/",
    "lint:fix": "npm run lint -- --fix",
    "seperator:util": "======================== UTILITY SCRIPTS =======================================",
    "prepare:dev": "cp index.html bundle/index.html && rexreplace 'bundle/' '' bundle/index.html",
    "compile:dev": "npm run prepare:dev && NODE_ENV='development' webpack-dev-server --progress --config webpack.config.dev.js",
    "compile:prod": "NODE_ENV='production' webpack -p --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/master-atul/master-atul.github.io.git"
  },
  "keywords": [
    "personal",
    "portfolio",
    "website"
  ],
  "author": "atulanand94@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/master-atul/master-atul.github.io/issues"
  },
  "homepage": "https://github.com/master-atul/master-atul.github.io#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "eslint": "^4.1.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "rexreplace": "^2.2.3",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "rc-touchable": "^1.2.2",
    "react": "16.0.0-alpha.13",
    "react-dom": "16.0.0-alpha.13",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.0",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rss-parser-browser": "^2.2.2"
  }
}
